{
    "analize": {
        "debug": {
            "decompile": false
        },
        "jdk-path": "Z:\\Oracle\\Java\\jdk-15.0.2\\bin",
        "javap-options": "-c -v -p -l",
        "work-path": "Z:\\workspace\\GitHub\\pyReverseCRUD\\resources\\projects\\sample002",
        "Project": {
            "path": "Z:\\workspace\\java\\sample002",
            "java-src": "\\src\\main\\java",
            "classes": "\\target\\classes",
            "mapper-xml": "\\src\\main\\resources\\dao"
        },
        "spring": {
            "controller": {
                "checkAnnotations": {
                    "RuntimeVisibleAnnotations:": ["org.springframework.stereotype.Controller"]
                },
                "isController": true,
                "isService": false,
                "isComponent": false,
                "isMapper": false
            },
            "service": {
                "checkAnnotations": {
                    "RuntimeVisibleAnnotations:": ["org.springframework.stereotype.Service"]
                },
                "isController": false,
                "isService": true,
                "isComponent": false,
                "isMapper": false
            },
            "component": {
                "checkAnnotations": {
                    "RuntimeVisibleAnnotations:": ["org.springframework.stereotype.Component"]
                },
                "isController": false,
                "isService": false,
                "isComponent": true,
                "isMapper": false
            },
            "mapper": {
                "checkAnnotations": {
                    "RuntimeVisibleAnnotations:": ["org.apache.ibatis.annotations.Mapper"]
                },
                "isController": false,
                "isService": false,
                "isComponent": false,
                "isMapper": true
            }
        },
        "source-re": {
            "class-re": "^(public\\s+class|public\\s+final\\s+class|public\\s+abstract\\s+class|class)\\s+(?P<fqcn>\\S+)",
            "interface-re": "^(public\\s+interface|interface)\\s+(?P<fqcn>\\S+)",
            "implements-re": "public\\s+class\\s+\\S+\\s+implements\\s+(?P<impl>\\S+)",
            "constructor-re": "(public|private)\\s+(?P<fqcn>\\S+)\\((?P<args>[^\\(\\)]*)\\);$",
            "method1-re": "(?P<scope>(public|private))\\s+(?P<ret>\\S+)\\s+(?P<method_name>\\S+)\\((?P<args>[^\\(\\)]*)\\);$",
            "method2-re": "(?P<scope>(public|private))\\s+abstract\\s+(?P<ret>\\S+)\\s+(?P<method_name>\\S+)\\((?P<args>[^\\(\\)]*)\\);$",
            "InterfaceMethodref-re": "\\s*(?P<ref_no>#\\d+)\\s*=\\s*InterfaceMethodref\\s*\\S+\\s*\\/\\/\\s*(?P<fqcn_method>[^\\.]+\\.\\w+)",
            "code-block-re": "\\{(?P<code>.+)\\}"
        }
    },
    "Excel": {
        "start_row": 1,
        "header_rows": 3,
        "start_column": 4,
        "crud_width": 3,
        "tables": {
            "master": "マスタ系",
            "transaction":"トラン系"
        }
    },
    "tables": {
        "master": [
            {"user": "ユーザマスタ"},
            {"syain": "社員マスタ"}
        ],
        "transaction": [
            {"products": "製品"},
            {"some_table": "トランザクションテーブル"}
        ]
    }
}